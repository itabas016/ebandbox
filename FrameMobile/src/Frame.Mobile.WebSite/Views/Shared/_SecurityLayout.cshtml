@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.title</title>

    <link href="@Url.Content("~/Content/news/css/main.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Calendar/WdatePicker.js")" type="text/javascript"></script>
</head>
<body>
    <div class="top">
        <img src="../../Content/common/images/mobile_logo.png" alt="安全配置管理" style="float: left;">
        @*<div style="float: right; line-height: 25px;">
            <span style="float: left; display: block; padding: 15px 15px 0 0">
                <a href="/Account/Manage?userName=@userName" style="color:white"><u>@userName</u></a>
            </span>
            <a href="/Account/LogOff" class="logout" title="退出登录"></a>
        </div>*@
    </div>
    <div class="main">
        <div class="left">
            <ul id="navOp">
                <li><span class="ico icon-layers"></span>@Html.ActionLink("浮窗禁用管理", "FloatingList", "SecurityUI", null, new { highlight = "true" })</li>
                <li><span class="ico icon-compass"></span>@Html.ActionLink("权限白名单", "WhiteList", "SecurityUI", null, new { highlight = "true", style = "color:white" })</li>
                <li><span class="ico icon-compass"></span>@Html.ActionLink("应用推荐权限配置", "AppPermissionList", "SecurityUI", null, new { highlight = "true", style = "color:white" })</li>
                </ul>
        </div>
        <div class="right">
            <div id="errorInfo" class="error">
                @if (TempData["errorMsg"] != null)
                {
                    @Html.Raw(TempData["errorMsg"].ToString())
                }
            </div>
            @RenderBody()
        </div>
    </div>
</body>
</html>
