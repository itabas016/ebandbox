<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/account/css/main.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
</head>
<body>
    <div>
        <div class="left">
            <ul>
                <li>@Html.ActionLink("修改资料", "ChangeInfo", "Account")</li>
                <li>@Html.ActionLink("修改密码", "ChangePassword", "Account")</li>
                <li>@Html.ActionLink("新闻管理", "NewsManage", "NewsUI")</li>
                <li>@Html.ActionLink("壁纸管理", "WallPaperManage", "WallPaperUI")</li>
                <li>@Html.ActionLink("手机属性管理", "MobileManage", "Mobile")</li>
                <li>@Html.ActionLink("退出", "LogOff", "Account")</li>
            </ul>
        </div>

    </div>
    <div class="right">
        <div class="top">
            用户管理中心
        </div>
        <div id="errorInfo" class="error">
            @if (TempData["errorMsg"] != null)
            {
                @Html.Raw(TempData["errorMsg"].ToString())
            }
        </div>
        @RenderBody()
    </div>
</body>
</html>
