<?xml version="1.0" encoding="utf-8"?>
<quartz xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 				version="1.0"
				overwrite-existing-jobs="true">

  <job>
    <job-detail>
      <name>TouTiaoJob</name>
      <group>TouTiao</group>
      <description></description>
      <job-type>TYDNewsService.TouTiaoJob,TYDNewsService</job-type>
      <volatile>false</volatile>
      <durable>true</durable>
      <recover>false</recover>
    </job-detail>
    <trigger>
      <cron>
        <name>every10min</name>
        <group>TouTiao</group>
        <job-name>TouTiaoJob</job-name>
        <job-group>TouTiao</job-group>
        <start-time>1900-01-01T00:00:00.0Z</start-time>
        <cron-expression>0 0/15 * * * ?</cron-expression>
      </cron>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>UpdateNewsJob</name>
      <group>UpdateNews</group>
      <description></description>
      <job-type>TYDNewsService.UpdateNewsJob,TYDNewsService</job-type>
      <volatile>false</volatile>
      <durable>true</durable>
      <recover>false</recover>
    </job-detail>
    <trigger>
      <cron>
        <name>every1Hour</name>
        <group>UpdateNews</group>
        <job-name>UpdateNewsJob</job-name>
        <job-group>UpdateNews</job-group>
        <start-time>1900-01-01T00:00:00.0Z</start-time>
        <cron-expression>0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
  </job>
  <job>
    <job-detail>
      <name>CleanJob</name>
      <group>Clean</group>
      <description></description>
      <job-type>TYDNewsService.CleanJob,TYDNewsService</job-type>
      <volatile>false</volatile>
      <durable>true</durable>
      <recover>false</recover>
    </job-detail>
    <trigger>
      <cron>
        <name>every1month</name>
        <group>Clean</group>
        <job-name>CleanJob</job-name>
        <job-group>Clean</job-group>
        <start-time>1900-01-01T00:00:00.0Z</start-time>
        <cron-expression>0 0 0 1 * ?</cron-expression>
      </cron>
    </trigger>
  </job>
</quartz>
